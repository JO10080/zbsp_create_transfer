<mvc:View controllerName="zbsp.bz.zbspcreatetransfer.controller.MenuTransfers"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <Page id="page" title="{i18n>title}">
        <content>
            <SelectList id="s2"
                items="{
                    path: 'menu>/ProductCollection'
                    
                }"
                itemPress="onSelect">
                <core:Item id="s" key="{menu>ProductId}" text="{menu>Name}" />
            </SelectList>
        </content>
    </Page>
</mvc:View>
