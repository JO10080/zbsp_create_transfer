<mvc:View controllerName="zbsp.bz.zbspcreatetransfer.controller.MenuNewTransactionChainTransfers"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
	xmlns:l="sap.ui.layout"
    xmlns:form="sap.ui.layout.form"
    >
	<!--1-->
    <HBox id="hboxchain1"
    alignItems="Center"
    justifyContent="Start">
    
		<Label  class="sapUiMediumMarginBegin sapUiTinyMarginEnd" required="true" id="labelchain1" text="Correspondent Bank:" labelFor="productInput" />
		<Input
			id="productChain1"
			showSuggestion="true"
			showValueHelp="true"
			valueHelpRequest=".onValueHelpRequest"
			suggestionItems="{/ProductCollection}"
            width="240%">
			<suggestionItems>
				<core:Item id="namechain1" text="{Name}" />
			</suggestionItems>
		</Input>
	
    </HBox>
<!--1-->
